{
  "name": "Search caniuse.com",
  "description": "Search caniuse.com from Chrome's context menu",
  "version": "0.1",
  "manifest_version": 3,
  "background": {
    "service_worker": "background.js"
  },
  "permissions": ["contextMenus"],
  "action": {
    "default_icon": {
      "16": "/images/caniuse128.png",
      "32": "/images/caniuse128.png",
      "48": "/images/caniuse128.png",
      "128": "/images/caniuse128.png"
    }
  },
  "icons": {
    "16": "/images/caniuse128.png",
    "32": "/images/caniuse128.png",
    "48": "/images/caniuse128.png",
    "128": "/images/caniuse128.png"
  }
}
